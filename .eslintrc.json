{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "new-cap": "off",
        "indent": ["warn", "tab"],
        "no-tabs": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": ["warn","always"]
    }
}
